    "maestro:version": "maestro --version",
    "maestro:doctor": "maestro doctor",
    "test:maestro": "maestro test maestro/flows/",
    "test:maestro:ios": "maestro test maestro/flows/ --platform ios",
    "test:maestro:android": "maestro test maestro/flows/ --platform android",
    "test:maestro:smoke": "maestro test maestro/flows/auth/login.yaml && maestro test maestro/flows/movies/discover.yaml",
    "test:maestro:auth": "maestro test maestro/flows/auth/login.yaml",
    "test:maestro:register": "maestro test maestro/flows/auth/register.yaml",
    "test:maestro:movies": "maestro test maestro/flows/movies/",
    "test:maestro:favorites": "maestro test maestro/flows/favorites/",
    "test:maestro:watchlist": "maestro test maestro/flows/watchlist/",
    "test:maestro:tabs": "maestro test maestro/flows/tabs/",
    "test:maestro:setup": "maestro test maestro/flows/setup/",
    "maestro:studio": "maestro studio",
    "maestro:record": "maestro record",
    "maestro:report": "maestro open-report",
    "maestro:check-id": "node maestro/scripts/check-app-id.js"
